{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "green-future-project-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/green-future-project-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": ["**/*.scss"],
                "output": "/assets/"
              }
            ],
            "optimization": {
              "scripts": false,
              "styles": {
                "minify": false,
                "inlineCritical": false
              },
              "fonts": false
            },
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/bootstrap/scss/bootstrap.scss",
              "./node_modules/aos/dist/aos.css",
              "src/assets/css/nucleo-icons.css",
              "src/assets/css/chivo.css",
              "node_modules/ngx-sharebuttons/themes/circles.scss",
              "src/styles.scss"
            ],
            "scripts": [
              
            ]
          },
          "configurations": {
            "production": {
              "outputPath": "dist/green-future-project-frontend-prod",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "index": {
                "input": "src/index.prod.html",
                "output": "index.html"

              },
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": false
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "100kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "green-future-project-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "green-future-project-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "green-future-project-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/aos/dist/aos.css",
              "src/assets/css/nucleo-icons.css",
              "src/assets/css/chivo.css",
              "src/styles.scss"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/@google/model-viewer/dist/model-viewer.min.js",
              "./node_modules/aos/dist/aos.js"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "green-future-project-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "green-future-project-frontend:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "d48536f1-ce44-43c8-a75e-f08d5c695242"
  }
}
